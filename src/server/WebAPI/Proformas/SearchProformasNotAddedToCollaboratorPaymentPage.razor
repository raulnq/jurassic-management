@using WebAPI.Infrastructure.SqlKata;
@using WebAPI.Infrastructure.Ui;

@foreach (var item in Result)
{
    <option value="@item.ProformaId">Proforma @item.Number, week @item.Week started at @item.Start.ToString("dd/MM/yyy") and ended At @item.End.ToString("dd/MM/yyy") with a subtotal of @item.Currency @item.SubTotal</option>
}

@code {
    [Parameter, EditorRequired]
    public List<SearchProformasNotAddedToCollaboratorPayment.Result> Result { get; set; } = default!;
}
